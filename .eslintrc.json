{
  "env": {
      "node": true,
      "browser": true,
      "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "react/prop-types": "off"
  }
}